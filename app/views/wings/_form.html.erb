<%= form_for(@wing) do |f| %>
    <%= f.text_field :style, placeholder: true %><br><br>

    <%= f.text_field :flavor, placeholder: true %><br><br>

    <%= f.label :restaurant_data %>
    <%= f.collection_select :restaurant_id, Restaurant.all, :id, :restaurant_data %><br><br>

    <%= f.submit "Submit Wing" %>

<% end %>