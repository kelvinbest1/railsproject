<div class="user_list">
    <h1>List of Users</h1>

    <h4>Click on a name to view their list of wings!</h4>
    <ul>
        <% @users.each do |user| %>
            <li><%= link_to user.name, user_wings_path(user.id) %></li><br>
        <% end %>
    </ul>
</div>

<%= form_tag('/search', method: 'get') do %>
    <%= label_tag 'name' %>
    <%= text_field_tag 'name' %>
    <%= submit_tag 'Submit' %>

<% end %>